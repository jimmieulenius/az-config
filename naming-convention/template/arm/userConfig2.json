{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {},
  "resources": [],
  "outputs": {
    "userConfig": {
      "type": "object",
      "value": {
        "format": {
          "$field": {
            "setAction": "combine"
          },
          "default": {
            "formatString": "{ENVIRONMENT}[{-}{ORGANIZATION}]{-}{PROJECT}[{PROJECT_INSTANCE}][{-}{SUBPROJECT}][{-}{REGION}]{-}{RESOURCE}[{RESOURCE_INSTANCE}][{SUBRESOURCE}][{SUBRESOURCE_INSTANCE}][{-}{UNIQUE}]"
          },
          "integration": {
            "formatString": "{ENVIRONMENT}{-}{PROJECT}[{-}{SUBPROJECT}]{-}{INTEGRATION_GROUP}{-}{INTEGRATION_NUMBER}[{-}{REGION}][{-}{INSTANCE}]{-}{RESOURCE_TYPE}[{-}{UNIQUE}]",
            "casing": "lower"
          }
        },
        "placeholders": {
          "PROJECT": {
            "$type": "lookup",
            "source": {
              "integrationFlow": {
                "value": "if"
              },
              "odata": {
                "value": "odata"
              },
              "rest": {
                "value": "rest"
              },
              "soap": {
                "value": "soap"
              }
            }
          },
          "SUBPROJECT": {
            "$type": "text"
          },
          "INTEGRATION_GROUP": {
            "$type": "text"
          },
          "INTEGRATION_NUMBER": {
            "$type": "text"
          },
          "ENVIRONMENT": {
            "$type": "lookup",
            "$field": {
              "setAction": "replace"
            },
            "source": {
              "dev": {
                "value": "d"
              },
              "qas": {
                "value": "q"
              },
              "prod": {
                "value": "r"
              },
              "run": {
                "value": "r"
              }
            }
          }
        }
      }
    }
  }
}
