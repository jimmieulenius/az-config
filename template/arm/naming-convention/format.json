{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {},
  "resources": [],
  "outputs": {
    "format": {
      "value": {
        "format": {
          "$default": {
            "value": "{ENVIRONMENT}{-}{ORGANIZATION}{-}{PROJECT}[{-}{SUBPROJECT}][{-}{INSTANCE}][{-}{REGION}]{-}{RESOURCE_TYPE}[{-}{UNIQUE}]",
            "separator": "-"
          },
          "deployment": {
            "value": "{ENVIRONMENT}{-}{ORGANIZATION}{-}{PROJECT}[{-}{SUBPROJECT}][{-}{INSTANCE}][{-}{REGION}][{-}{RESOURCE_TYPE}][{-}v{VERSION}][{-}{DEPLOYMENT_PHASE}][{-}{SUFFIX}][{-}{UNIQUE}]"
          },
          "storageAccount": {
            "separator": {
              "$value": null
            },
            "length": 24,
            "casing": "lower"
          },
          "subnet": {
            "value": "{ENVIRONMENT}-{PROJECT}-{INSTANCE}[{-}{UNIQUE}]"
          }
        },
        "$instance": {
          "maxValue": 99,
          "padding": {
            "totalLength": 2,
            "character": "0"
          }
        },
        "$unique": {
          "length": 4,
          "seed": null
        }
      },
      "type": "object"
    }
  }
}
